p = .70, list = FALSE)
ams.training <- nhoods[inTrain,]
ams.test <- nhoods[-inTrain,]
ams.training <- ams.training%>%
dplyr::select(price, postcode4, geometry,highrise_nn3,greenroof_nn3,wallart_nn3,market_nn3,swimming_nn3,pct_dutch_bkg,pct_nonwes_bkg,avr_fam_size,low_income_pct  )
ams.test <- ams.test%>%
dplyr::select(price, postcode4, geometry,highrise_nn3,greenroof_nn3,wallart_nn3,market_nn3,swimming_nn3,pct_dutch_bkg,pct_nonwes_bkg,avr_fam_size,low_income_pct )
reg.training <-
lm(price ~ ., data = as.data.frame(ams.training) %>%
dplyr::select(price, highrise_nn3,greenroof_nn3,wallart_nn3,market_nn3,swimming_nn3,pct_dutch_bkg,pct_nonwes_bkg,avr_fam_size,low_income_pct ))
ams.test <-
ams.test %>%
na.omit()
ams.test <- ams.test %>%
mutate(Regression = "Baseline Regression",
listing_price.Predict = predict(reg.training, ams.test),
listing_price.Error = listing_price.Predict - price,
listing_price.AbsError = abs(listing_price.Predict - price),
listing_price.APE = (abs(listing_price.Predict - price)) / listing_price.Predict)
ams.test %>%
st_drop_geometry() %>%
summarise(MAE = mean(listing_price.AbsError),
MAPE = mean(abs(listing_price.APE)*100)) %>%
kbl(col.name=c('Mean Absolute Error','Mean Absolute Percentage Error')) %>%
kable_classic()
View(ams.test)
ggplot(ams.test)+
geom_sf(aes(colour = listing_price.AbsError))
scale_fill_viridis()+
labs(title = "MAE distribution") +
mapTheme()
ggplot(ams.test)+
geom_sf(aes(colour = listing_price.AbsError))+
scale_fill_viridis()+
labs(title = "MAE distribution") +
mapTheme()
library(viridis)
ggplot(ams.test)+
geom_sf(aes(colour = listing_price.AbsError))+
scale_fill_viridis()+
labs(title = "MAE distribution") +
mapTheme()
ggplot(ams.test)+
geom_sf(aes(colour = listing_price.AbsError))+
geom_sf(data = district,fill = NA)+
scale_fill_viridis()+
labs(title = "MAE distribution") +
mapTheme()
